@*Build pagination at the bottom of the page*@
@*<div page-x="@Model.PageInfo" page-action="Index"></div>*@



@model BooksViewModel
@foreach (var b in Model.Books)
{
    <partial name="BookSummary" model="b"/>
}
<div page-x="@Model.PageInfo" page-action="Index" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" class="btn-group pull-right m-1">
</div>
